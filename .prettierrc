{
  "arrowParens": "always",
  "bracketSpacing": true,
  "printWidth": 400,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 4,
  "overrides": [
          {
                  "files": "*.vimrc",
                  "options": {
                          "tabWidth": 4
                  }
          }
  ],
  "trailingComma": "es5",
  "useTabs": false,
  "proseWrap": "never"
}
